              </div>
             {%- endif -%}             
               {% assign variant = product.selected_or_first_available_variant %}               
               {% if variant.metafields.custom.tab_title_1 != blank %}
                 <div class="tab-title" data-target="dynamic_content_1">
                    {{ variant.metafields.custom.tab_title_1 }}
                 </div>
               {% endif %}
            {% if variant.metafields.custom.tab_title_2 != blank %}
                 <div class="tab-title" data-target="dynamic_content_2">
                    {{ variant.metafields.custom.tab_title_2 }}
                 </div>
               {% endif %}
            {% if variant.metafields.custom.tab_title_3 != blank %}
                 <div class="tab-title" data-target="dynamic_content_3">
                    {{ variant.metafields.custom.tab_title_3 }}
                 </div>
               {% endif %}
            {% if variant.metafields.custom.tab_title_4 != blank %}
                 <div class="tab-title" data-target="dynamic_content_4">
